function func(arr,x,y){
  let sum = 0;
  
  for(let i = x-2;i<=x+2;i++){
    for(let j=y-2 ;j<=y+2;j++){
      if(i>=0 && i<=arr.length-1 && j>=0 && j<=arr.length-1 ){
        sum += arr[i][j];
      }
    }
  }
  
  console.log(sum);
  
}
arr=[[1,2,3,4],[5,6,7,8],[13,14,15,16],[17,18,19,20]];
func(arr,0,1);
